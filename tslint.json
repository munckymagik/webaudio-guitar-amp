{
    "extends": "tslint:latest",
    "rules": {
      "member-access": false,
      "no-console": false,
      "quotemark": [true, "single", "avoid-escape"],
      "semicolon": [ true, "never" ],
      "trailing-comma": false,
      "variable-name": [true,
        "ban-keywords",
        "check-format",
        "allow-pascal-case",
        "allow-leading-underscore"
      ]
    }
}
